{
	"name": "photophobia",
	"version": "0.1.0",
	"description": "A boring dark theme",
	"repository": "https://github.com/setsevireon/photophobia.git",
	"author": "setsevireon",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "postcss --use autoprefixer --use tailwindcss --use cssnano -d assets/output/css/ assets/css/**/*.css",
		"serve": "hugo server --config config-dev.yaml --source demo --debug",
		"unserve": "pkill hugo",
		"clean": "rm -fr public demo/public server.log"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.19",
		"cssnano": "^7.0.5",
		"postcss": "^8.4.38",
		"postcss-cli": "^11.0.0",
		"tailwindcss": "^3.4.3"
	}
}
